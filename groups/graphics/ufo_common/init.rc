# for hardware accelerated graphics
service ufo-init /system/bin/sh /system/etc/init.ufo.sh
    class core
    oneshot

service coreu /system/bin/coreu
    class main
    disabled

on property:ro.ufo.use_coreu=1
    start coreu

on post-fs-data
    mkdir /data/system 0770 system system
    restart ufo-init

